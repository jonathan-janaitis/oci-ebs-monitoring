        <SQLQuery>
            <![CDATA[select 'ME$EBS_ATG_WF_CLOUD_MAILER_SMTP_CONNECTION_SECURITY_INCORRECT' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, 'dummyalias:' || s.dummyalias key_data FROM (SELECT      dummy dummyalias  FROM      dual,      (          SELECT              COUNT(*) count_test          FROM              ( (                  SELECT                      1                  FROM                      dual                  WHERE                      EXISTS (                          SELECT                              c.parameter_name,                              a.parameter_id,                              c.display_name,                              a.parameter_value                          FROM                              apps.fnd_svc_comp_param_vals a,                              apps.fnd_svc_comp_params_vl  c                          WHERE                                  c.parameter_id = a.parameter_id                              AND a.component_id = 10006                              AND c.parameter_name ='OUTBOUND_SERVER'                               AND a.parameter_value='smtp.office365.com'                       )                  UNION ALL                  SELECT                      1                  FROM                      dual                  WHERE                      EXISTS (                          SELECT                              c.parameter_name,                              a.parameter_id,                              c.display_name,                              a.parameter_value                          FROM                              apps.fnd_svc_comp_param_vals a,                              apps.fnd_svc_comp_params_vl  c                          WHERE                                  c.parameter_id = a.parameter_id                              AND a.component_id = 10006                              AND c.parameter_name='OUTBOUND_SECURE_PROTOCOL'                               AND a.parameter_value != 'STARTTLS'                      )              ) ) test_a          HAVING              COUNT(*) = 2      ) test_b  WHERE      count_test = 2) s]]>
