    <DisplayName>EBS Sensor: FIN AP Payment accounting is incorrect because bank charges are applied - Weekly</DisplayName>
    <Description><![CDATA[Payment accounting is incorrect because bank charges are applied even though the bank and payment currencies are the same. Result is a 95353 error during a]]></Description>
    <IsSystem>0</IsSystem>
    <IsSecureContent>1</IsSecureContent>
    <Patterns>
        <DBPattern dateTimeField="entrytime" dateTimeColumns="entrytime">
        <SQLQuery>
            <![CDATA[select 'ME$EBS_FIN_AP_MU_INVALID_BANK_CHARGE' sensor, to_timestamp(to_char(sysdate, 'YYYY-MM-DD hh24:mi:ss'), 'YYYY-MM-DD hh24:mi:ss') entrytime, 'check_id:' || s.check_id key_data FROM (SELECT      check_id check_id  FROM      (          WITH chks AS (              SELECT /*+ materialize */ DISTINCT                  ph.check_id              FROM                  APPS.ap_payment_history_all ph              WHERE                  ph.accounting_date BETWEEN ( trunc(sysdate) - 7 ) AND ( trunc(sysdate) + 3 )          )  SELECT aph.check_id,            aph.payment_history_id,            aph.accounting_event_id,            aph.transaction_type,            aph.accounting_date,            aph.posted_flag,            aph.org_id,            aph.charges_bank_amount,            aph.trx_bank_amount,            aph.trx_pmt_amount,            aph.bank_currency_code,            aph.pmt_currency_code     FROM  chks,          APPS.ap_payment_history_all aph,          APPS.ap_system_parameters_all asp     WHERE aph.check_id = chks.check_id     AND   aph.transaction_type IN ('PAYMENT CLEARING', 'PAYMENT UNCLEARING')     AND   aph.bank_currency_code = aph.pmt_currency_code     AND   aph.org_id = asp.org_id     AND   aph.bank_currency_code = asp.base_currency_code     AND   nvl(CHARGES_BANK_AMOUNT,0) ! = 0          )  WHERE      ROWNUM < 11  ORDER BY      check_id  ) s]]>
        </SQLQuery>
        <IsInclude>1</IsInclude>
            <InUse>1</InUse>
            <DBQueryFieldList>
                <DBPatternSeqField>entrytime</DBPatternSeqField>
                <DBPatternField>
